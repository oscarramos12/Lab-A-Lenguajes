let delim = ['\n']
let ws = delim+
let digit = ['0'-'9']
let number = digit+

rule tokens = 
    ws        { return ws}
  | number    { return NUMBER }
  | '+'       { return PLUS }
  | '*'       { return TIMES }
  | '('       { return LPAREN }
  | ')'       { return RPAREN }

